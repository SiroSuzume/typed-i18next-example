{
  "name": "typed-i18next-example",
  "version": "0.1.0",
  "description": "Next + i18next + typescript example",
  "engines": {
    "node": ">=14.17.1",
    "yarn": ">=1.22.5"
  },
  "author": "Sirosuzume",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all -s lint:eslint prettier:check",
    "lint:eslint": "eslint . --max-warnings=0 --ext .ts,.tsx,.js --ignore-path .gitignore",
    "format": "npm-run-all -s fix:eslint prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "prettier": "prettier -w *.js '**/*.{js,ts,tsx,css}' --ignore-path .gitignore",
    "prettier:check": "prettier -c '*.js' '**/*.{js,ts,tsx,css}' --ignore-path .gitignore",
    "test": "jest --passWithNoTests",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "date-fns": "^2.22.1",
    "framer-motion": "^4.1.17",
    "i18next": "^20.3.2",
    "next": "^11.0.1",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.0",
    "react-icons": "^4.2.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@chakra-ui/babel-plugin": "^1.0.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@jest/types": "^27.0.6",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@types/eslint": "^7.2.13",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/react": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
